Prefix(swrl:=<http://www.w3.org/2003/11/swrl#>)
Prefix(owl:=<http://www.w3.org/2002/07/owl#>)
Prefix(rdf:=<http://www.w3.org/1999/02/22-rdf-syntax-ns#>)
Prefix(var2:=<urn:http://www.w3.org/2003/11/swrl#>)
Prefix(var:=<urn:swrl#>)
Prefix(xsd:=<http://www.w3.org/2001/XMLSchema#>)
Prefix(swrlb:=<http://www.w3.org/2003/11/swrlb#>)
Prefix(:=<http://com.github.aites/ontologies/smarthome.owl#>)
Prefix(xml:=<http://www.w3.org/XML/1998/namespace>)
Prefix(rdfs:=<http://www.w3.org/2000/01/rdf-schema#>)
Ontology(<http://com.github.aites/ontologies/smarthome.owl#>
Declaration(Class(:OverPower))
DLSafeRule(Annotation(rdfs:comment "Rule for judge CCTV overpower condition")Body(ClassAtom(:Cctv Variable(var:p)) DataProperty(:power_consumtion Variable(var:p) Variable(var:power)) BuiltInAtom(swrlb:greaterThan Variable(var:power) "120"^^xsd:integer) )Head(ClassAtom(:OverPower Variable(var:p))))
Declaration(Class(:UnderPower))
DLSafeRule(Annotation(rdfs:comment "Rule for judge CCTV underpower condition")Body(ClassAtom(:Cctv Variable(var:p)) DataProperty(:power_consumtion Variable(var:p) Variable(var:power)) BuiltInAtom(swrlb:lessTan Variable(var:power) "20"^^xsd:integer) )Head(ClassAtom(:UnderPower Variable(var:p))))
Declaration(Class(:StatusEqualAct))
DLSafeRule(Annotation(rdfs:comment "Rule about status act ")Body(ClassAtom(:Device Variable(var:p)) DataPropertyAtom(:power_consumtion Variable(var:p) Variable(var:power)) BuiltInAtom(swrlb:equal Variable(var:stat) "act"^^xsd:string) )Head(ClassAtom(:StatusEqualAct Variable(var:p))))
)
