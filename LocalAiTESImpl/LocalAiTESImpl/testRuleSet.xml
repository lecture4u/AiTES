Declaration(Class(:OverPower))
DLSafeRule(Annotation(rdfs:comment "Rule for judge CCTV owerfower condition")Body(ClassAtom(:Cctv Variable(var:p)) DataPropertyAtom(:power_consumtion Variable(var:p) Variable(var:power)) BuiltInAtom(swrlb:greaterThan Variable(var:power) "120"^^xsd:integer))Head(ClassAtom(:OverPower Variable(var:p))))
Declaration(Class(:UnderPower))
DLSafeRule(Annotation(rdfs:comment "Rule for judge CCTV underpower condition")Body(ClassAtom(:Cctv Variable(var:p)) DataPropertyAtom(:power_consumtion Variable(var:p) Variable(var:power)) BuiltInAtom(swrlb:lessTan Variable(var:power) "20"^^xsd:integer))Head(ClassAtom(:UnderPower Variable(var:p))))
Declaration(Class(:StatusEqualAct))
DLSafeRule(Annotation(rdfs:comment "Rule about status act ")Body(ClassAtom(:Device Variable(var:p)) BuiltInAtom(swrlb:equal Variable(var:stat) "act"^^xsd:string))Head(ClassAtom(:StatusEqualAct Variable(var:s))))
